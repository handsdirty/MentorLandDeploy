<h1>Listing users</h1>

<table>
  <tr>
    <th>Email</th>
    <th>Name</th>
    <th>Password digest</th>
    <th>First name</th>
    <th>Last name</th>
    <th>Credit card no</th>
    <th>Credit card month</th>
    <th>Credit card year</th>
    <th>Credit card verification value</th>
    <th>Credit card billing address</th>
    <th>Credit card type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.email %></td>
    <td><%= user.name %></td>
    <td><%= user.password_digest %></td>
    <td><%= user.first_name %></td>
    <td><%= user.last_name %></td>
    <td><%= user.credit_card_no %></td>
    <td><%= user.credit_card_month %></td>
    <td><%= user.credit_card_year %></td>
    <td><%= user.credit_card_verification_value %></td>
    <td><%= user.credit_card_billing_address %></td>
    <td><%= user.credit_card_type %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
